N=load('N/N_3.txt');
T=load('T/T_3.txt');

T1=[T zeros(size(T,1),1)]
for t=1:size(T,1)
if (mean(N(T(t,:),4))>-300)
    T1(t,4)=0;
else if 
    
end
dlmwrite('myFile.txt', T1,'delimiter','\t');

return
close all
clear all
clc

dim=2;

meshID=3;
%x=rand(1,2)*30
x=[-0.05 -0.15]

system(['./search ' num2str(meshID) ' ' num2str(x(1)) ' ' num2str(x(2))])

P=load('P/P_0.txt');
