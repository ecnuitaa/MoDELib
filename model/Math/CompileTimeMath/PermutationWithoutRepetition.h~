#ifndef mmdl_PERMUTATIONWITHOUTREPETITION_H_
#define mmdl_PERMUTATIONWITHOUTREPETITION_H_


#include <mmdl/Math/CompileTimeMath/Factorial.h>

namespace mmdl {
	
	
	template <int N, int k>
	struct PermutationWithoutRepetition{
		enum{value=Factorial<N>::value / Factorial<(k>=0 && k<=N && N>=0)?(N-k):0>::value * (k>=0 && k<=N && N>=0)};
	};
	
	//////////////////////////////////////////////////////////////////////////
} // end namespace ctmath

#endif
